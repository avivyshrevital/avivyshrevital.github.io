          "_anchor": "text1-4n",
          "_params": {
            "paddingTop": "0",
            "paddingBottom": "1",
            "showTitle": false,
            "showText": true,
            "bgImageRadio": true,
            "bgImage": "@PROJECT_PATH@/assets/images/epiphany-stuart-land-ebook-cover-2000x2994.jpg",
            "parallax": true,
            "bgColorRadio": false,
            "bgColor": "#fff",
            "bgVideoRadio": false,
            "bgVideo": {
              "url": "http://www.youtube.com/watch?v=uNCr7NdOJgw"
